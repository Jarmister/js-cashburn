{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12
    },
    "plugins": [
        "react",
        "@typescript-eslint",
        "prettier"
    ],
    "rules": { "camelcase": "off",
                "no-eval":"off",
                "react/jsx-uses-react": "error",
                "react/jsx-uses-vars": "error",
                "spaced-comment": "off"
                
      

    
      
      
        
    }


}